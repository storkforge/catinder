input UserInput {
    userFullName: String!
    userName: String!
    userEmail: String!
    userLocation: String!
    userRole: String!
    userAuthProvider: String!
}

input UserUpdate {
    userFullName: String
    userName: String
    userEmail: String
    userLocation: String
    userRole: String
    userAuthProvider: String
}

type UserOutputDTO {
    userId: ID!
    userFullName: String
    userName: String!
    userEmail: String!
    userLocation: String
    userRole: String
    userAuthProvider: String
}


type UserConnection {
    edges: [UserEdge]
    pageInfo: PageInfo!
}

type UserEdge {
    cursor: String!
    node: UserOutputDTO
}

type PageInfo {
    hasNextPage: Boolean!
    endCursor: String
}
